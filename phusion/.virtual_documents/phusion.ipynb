def Tm_NEB(primer):
    tm = mt.Tm_NN( primer,
               nn_table=mt.DNA_NN3,
               Na=50,
               Tris=75.0,
               Mg=1.5, 
               dnac1=500, 
               dnac2=500, 
               dNTPs=0.8, 
               saltcorr=1)
    return tm


def Ta_NEB(fp,rp):
    tms = []
    for primer in [fp,rp]:
        tms.append(Tm_NEB(primer))
    tm = min(tms) + 3.0
    return tm if tm<72 else 72

