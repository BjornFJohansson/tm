


import Bio
from Bio.SeqUtils import MeltingTemp as mt
print("version", Bio.__version__)

tm = mt.Tm_NN("ATGGCAGTTGAGAAGA", 
       nn_table=mt.DNA_NN3,
       Na=50,
       Tris=0.0,
       Mg=0, 
       dnac1=25, 
       dnac2=25, 
       dNTPs=0, 
       saltcorr=5)

print(round(tm, 1))








